{
  "extends": [
    "eslint:recommended",
    "airbnb",
    "next/core-web-vitals",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime"
  ],
  "rules":  {
    "react/jsx-filename-extension": [2, { "extensions": [".jsx", ".tsx"] }],
    "import/extensions": [".js", ".jsx", ".ts", ".tsx"]
  }
}
